<%  if user.image.attached? %>
  <%= image_tag user.image.variant(resize: "200x200"), class: class_name, id: id_name %>
<% else %>
  <% if user.image_url? %>
    <img  id="<%= id_name %>" class="<%= class_name %>" src="<%= user.image_url %>" alt="twitterアイコン">
  <% else %>
    <img id="<%= id_name %>" class="<%= class_name %>" src="<%= asset_pack_path 'media/src/images/defoult-user-icon.png' %>" />
  <% end %>
<% end %>